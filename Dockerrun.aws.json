"AWSEBDockerrunVersion":2,
"containerDefinitions": [
    {
        "name": "client",
        "image": "jagasan/multi-client",
        "hostname": "client",
        "essential": false,
        "memory":128
    },
    {
        "name": "server",
        "image": "jagasan/multi-server",
        "hostname": "api",
        "essential": false,
        "memory":128
    },
    {
        "name": "worker",
        "image": "jagasan/multi-worker",
        "hostname": "worker",
        "essential": false,
        "memory":128
    },
    {
        "name": "nginx",
        "image": "jagasan/multi-nginx",
        "essential": true,
        "memory":128,
        "portMapping":[
            {
                "hostPort":80,
                "containerPort":80
            }
        ],
        "links": ["client","server"]
    }
]